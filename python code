#make you know the path for where you have saved the SIC_to_Fama_French_Industry.csv

def sic_to_ff(a,b):
    #import pandas package
    import pandas as pd
    #load data - make sure that the data has a column called "sic" which includes the SIC codes
    sic_codes = a
    sic_codes=data.dropna()
    ff_codes=pd.read_csv(b)
    results = pd.merge(sic_codes,ff_codes[["SIC","FF_12"]],left_on="sic",right_on="SIC",how="left")
    results.pop("SIC")
    return results
