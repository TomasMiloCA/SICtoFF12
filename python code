    sic_codes=pd.DataFrame(data["sic"])

    sic_codes["sic"]

    ff_codes["SIC"].iloc[1]


    ff_codes=pd.read_csv(r"C:\Users\tmilo\OneDrive - McGill University\Documents\Summer 2022\XBRL project\institutional_holdings\SIC_to_Fama_French_industry.csv")
    results = pd.merge(sic_codes,ff_codes[["SIC","FF_12"]],left_on="sic",right_on="SIC",how="left")
    return results
